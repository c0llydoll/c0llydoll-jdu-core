{
  "kind": "collectionType",
  "collectionName": "playlists",
  "info": {
    "singularName": "playlist",
    "pluralName": "playlists",
    "displayName": "Playlist",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "playlistId": {
      "pluginOptions": {},
      "type": "customField",
      "customField": "plugin::field-uuid.uuid"
    },
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": true
    },
    "songs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::song.song"
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true,
      "unique": true
    },
    "skus": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sku.sku"
    },
    "cover": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    }
  }
}
